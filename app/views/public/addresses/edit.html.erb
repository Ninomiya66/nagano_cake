<div class="container">

  <h1 class="mt-3 mb-3">配送先編集</h1>

  <%= form_with model: @address, url: address_path, method: :patch do |f| %>

  <div class="form-group">

    <%= f.label :"郵便番号(ハイフンなし)" %></br>
    <%= f.text_field :postal_code %></br>

  </div>

  <div class="form-group">

    <%= f.label :住所 %></br>
    <%= f.text_field :address %></br>

  </div>

  <div class="from-group">

    <%= f.label :宛名 %></br>
    <%= f.text_field :name %></br>

  </div>

  <div class="form-group">

    <%= f.submit "保存", class: "btn btn-outline-success mt-3 mb-3" %></br>

  </div>

  <% end %>

</div>