<h1>配送先編集</h1>

<%= form_with model: @address, url: address_path, method: :patch do |f| %>

  <%= f.label :"郵便番号(ハイフンなし)" %></br>
  <%= f.text_field :postal_code %></br>

  <%= f.label :住所 %></br>
  <%= f.text_field :address %></br>

  <%= f.label :宛名 %></br>
  <%= f.text_field :name %></br>

  <%= f.submit "保存" %></br>

<% end %>