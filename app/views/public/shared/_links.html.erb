<%- if controller_name != 'sessions' %>

  <%= link_to "Log in", new_session_path(resource_name), class: "btn-outline-info ml-2"%><br />

<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>

  <%= link_to "Sign in", new_registration_path(resource_name), class: "btn-outline-info mt-2" %></br>

<% end %>
