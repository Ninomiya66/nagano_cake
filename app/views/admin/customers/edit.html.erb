<h1>会員情報編集</h1>

<%= form_with model: @customer, url: admin_customer_path do |f| %>

  <%= f.label "会員ID" %>

  <%= f.label "#{@customer.id}" %>


  <%= f.label "氏名" %>

  <%= f.text_field :last_name %>

  <%= f.text_field :first_name %>


  <%=f.label "フリガナ" %>

  <%= f.text_field :last_name_kana %>

  <%= f.text_field :first_name_kana %>

<% end %>