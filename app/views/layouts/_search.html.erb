<%= form_with url: admin_search_path, method: :get do |f| %>

  <div class="d-flex justify-content-end mt-3 mr-5">

    <%= f.text_field :content, placeholder: "Search"%>

    <%= f.select :model, options_for_select({"Customer" => "customer","Item" => "item"})%>

    <%= f.select :method, options_for_select({"完全一致" => "perfect", "部分一致" => "partial"})%>

    <%= f.submit "検索", class:"btn btn-outline-secondary ml-2"%>

  </div>

<% end %>